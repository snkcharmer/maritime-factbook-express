# Environment Configuration for Maritime Factbook Express

# Server Configuration
PORT=5000                    # Port on which the Express server will run
NODE_ENV=development          # Set to 'production' in production environments

# Database Configuration
MONGO_URI=your_mongo_connection_string_here    # MongoDB connection string (e.g., mongodb+srv://username:password@cluster.mongodb.net/database)

# JWT Authentication
JWT_SECRET=your_jwt_secret_here      # Secret key for JWT signing and verification

# API Key
API_KEY=your_api_key_here             # API Key for external services
